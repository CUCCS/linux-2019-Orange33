{
  "redirect_rule":[{
      "to_uri":"/verynginx/index.html",
      "matcher":"demo_other_verynginx_uri",
      "enable":true
    }],
  "filter_rule":[{
      "enable":false,
      "matcher":"localhost",
      "action":"accept"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    },{
      "enable":true,
      "code":"403",
      "response":"ip_response",
      "action":"block",
      "custom_response":true,
      "matcher":"ip_reject"
    },{
      "enable":true,
      "code":"403",
      "response":"wp_response",
      "action":"block",
      "custom_response":true,
      "matcher":"wp_vulnerability"
    },{
      "enable":true,
      "code":"403",
      "response":"dvwa_sql_response",
      "matcher":"dvwa_sql_injection",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"403",
      "response":"vn_admin_response",
      "matcher":"vn_whitelist",
      "custom_response":true,
      "action":"block"
    },{
      "enable":true,
      "code":"403",
      "response":"curl_response",
      "action":"block",
      "custom_response":true,
      "matcher":"curl_forbidden"
    },{
      "enable":true,
      "code":"403",
      "response":"dvwa_response",
      "matcher":"dvwa_whitelist",
      "custom_response":true,
      "action":"block"
    }],
  "config_version":"0.36",
  "static_file_rule":[],
  "proxy_pass_rule":[{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wp",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"wordpress",
      "upstream":"wp_https",
      "proxy_host":""
    },{
      "enable":true,
      "matcher":"dvwa",
      "upstream":"dvwa",
      "proxy_host":""
    }],
  "summary_collect_rule":[],
  "summary_group_persistent_enable":true,
  "admin":[{
      "user":"verynginx",
      "password":"verynginx",
      "enable":true
    }],
  "dashboard_host":"",
  "response":{
    "curl_response":{
      "content_type":"text/html",
      "body":"Accessing via curl is NOT allowed!!!"
    },
    "frequency_limit":{
      "content_type":"text/html",
      "body":"Your requests was too frequent!!!"
    },
    "dvwa_sql_response":{
      "content_type":"text/html",
      "body":"Don't try to do SQL injection!!!"
    },
    "vn_admin_response":{
      "content_type":"text/html",
      "body":"You are not allowed to access this page!!!"
    },
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    },
    "dvwa_response":{
      "content_type":"text/html",
      "body":"You are not allowed to access this website!!!"
    },
    "ip_response":{
      "content_type":"text/html",
      "body":"Accessing via IP is NOT allowed!!!"
    },
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    },
    "wp_response":{
      "content_type":"text/html",
      "body":"Don't try to make use of this vulnerability!!!"
    }
  },
  "scheme_lock_enable":false,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "redirect_enable":true,
  "backend_upstream":{
    "wp":{
      "node":{
        "wp":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"8181"
        }
      },
      "method":"random"
    },
    "wp_https":{
      "node":{
        "wp_https":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"https",
          "port":"4433"
        }
      },
      "method":"random"
    },
    "dvwa":{
      "node":{
        "dvwa":{
          "host":"127.0.0.1",
          "weight":"1",
          "scheme":"http",
          "port":"8082"
        }
      },
      "method":"random"
    }
  },
  "frequency_limit_enable":true,
  "matcher":{
    "wp_vulnerability":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"="
      },
      "URI":{
        "value":"/wp-json/wp/v2/users/",
        "operator":"="
      }
    },
    "demo_verynginx_short_uri":{
      "URI":{
        "operator":"≈",
        "value":"^/vn"
      }
    },
    "localhost":{
      "IP":{
        "operator":"=",
        "value":"127.0.0.1"
      }
    },
    "vn_whitelist":{
      "Host":{
        "value":"vn.sec.cuc.edu.cn",
        "operator":"="
      },
      "IP":{
        "value":"192.168.56.4",
        "operator":"!="
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "operator":"=",
        "value":"/redirect_to_verynginx"
      }
    },
    "wordpress":{
      "Host":{
        "value":"wp.sec.cuc.edu.cn",
        "operator":"≈"
      }
    },
    "dvwa_sql_injection":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      },
      "Args":{
        "operator":"≈",
        "name_operator":"*",
        "value":"(select|or|and|union|order)"
      }
    },
    "attack_scan_0":{
      "UserAgent":{
        "operator":"≈",
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)"
      }
    },
    "all_request":{
    },
    "ip_reject":{
      "Host":{
        "value":"192.168.56.3",
        "operator":"="
      }
    },
    "dvwa_whitelist":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      },
      "IP":{
        "value":"192.168.56.4",
        "operator":"!="
      }
    },
    "verynginx":{
      "URI":{
        "operator":"≈",
        "value":"^/verynginx/"
      }
    },
    "attack_sql_0":{
      "Args":{
        "name_operator":"*",
        "operator":"≈",
        "value":"select.*from"
      }
    },
    "attack_code_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(git|svn|\\.)"
      }
    },
    "dvwa":{
      "Host":{
        "value":"dvwa.sec.cuc.edu.cn",
        "operator":"="
      }
    },
    "curl_forbidden":{
      "UserAgent":{
        "value":"curl",
        "operator":"≈"
      }
    },
    "attack_backup_0":{
      "URI":{
        "operator":"≈",
        "value":"\\.(htaccess|bash_history|ssh|sql)$"
      }
    }
  },
  "static_file_enable":true,
  "summary_temporary_period":60,
  "summary_group_temporary_enable":true,
  "summary_with_host":false,
  "frequency_limit_rule":[{
      "enable":true,
      "separate":["ip"],
      "code":"429",
      "response":"frequency_limit",
      "count":"49",
      "matcher":"dvwa",
      "custom_response":true,
      "time":"1"
    },{
      "enable":true,
      "separate":["ip"],
      "code":"429",
      "response":"frequency_limit",
      "count":"19",
      "matcher":"wordpress",
      "custom_response":true,
      "time":"1"
    }],
  "uri_rewrite_enable":true,
  "base_uri":"/verynginx",
  "summary_request_enable":true,
  "proxy_pass_enable":true,
  "readonly":false,
  "browser_verify_rule":[]
}